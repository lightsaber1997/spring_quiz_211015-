<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

 <mapper namespace="com.example.lesson02.dao.StoreDAO">
    <select id="selectStoreList"  resultType="com.example.lesson02.model.Store">
        SELECT
            *
        FROM
            store
    </select>
    
    <select id="selectById"  resultType="com.example.lesson02.model.Store">
        SELECT
            *
        FROM
            store
        WHERE
        	id = #{id}
    </select>

 </mapper>