<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.lesson05.dao.NewReviewDAO">
	<select id="selectByStoreId" parameterType="int"
		resultType="com.example.lesson05.model.NewReview">
		SELECT
			*
		FROM
			new_review
		where
			storeId = #{storeId}

	</select>
</mapper>